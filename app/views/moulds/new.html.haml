.col-md-8.col-md-offset-2
  .row
    .panel.panel-default
      .panel-heading
        %h1 Upload mould
      .panel-body
        = form_for @mould, html: { multipart: true } do |f|
          =f.file_field :image
          =f.label :name
          =f.text_field :name
          =f.submit 'Create Mould'
